<idea-plugin version="2">
  <id>com.agilezhu.bundledatabuilder</id>
  <name>BundleDataBuilder</name>
  <version>1.1</version>
  <vendor email="agilezhu@qq.com" url="https://github.com/guangdeshishe/BundleDataBuilder">AgileZhu</vendor>

  <description>
    generate [BundleDataBuilder] class code
    detail see:https://github.com/guangdeshishe/BundleDataBuilder
  </description>

  <change-notes>
    allow generage code auto
  </change-notes>

  <!-- please see http://www.jetbrains.org/intellij/sdk/docs/basics/getting_started/build_number_ranges.html for description -->
  <!--<idea-version since-build="141.0"/>-->
  <idea-version since-build="141.0"/>

  <!-- please see http://www.jetbrains.org/intellij/sdk/docs/basics/getting_started/plugin_compatibility.html
       on how to target different products -->
  <!-- uncomment to enable plugin in all products
  <depends>com.intellij.modules.lang</depends>
  -->

  <extensions defaultExtensionNs="com.intellij">
    <!-- Add your extensions here -->
  </extensions>

  <actions>
    <!-- Add your actions here -->
    <action id="menu_action_generate" class="com.agilezhu.bundledatabuilder.GenarateCodeAction" text="BundleDataBuilder"
            description="GenerateBundleDataBuilderCode">
      <add-to-group group-id="GenerateGroup" anchor="last"/>
    </action>
  </actions>

</idea-plugin>